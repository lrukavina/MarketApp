<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sample.CartController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <bottom>
            <TableView fx:id="itemTableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="itemNameColumn" prefWidth="226.0" text="Name:" />
                <TableColumn fx:id="itemTypeColumn" prefWidth="128.0" text="Type:" />
                  <TableColumn fx:id="itemQuantityColumn" prefWidth="96.0" text="Quantity:" />
                  <TableColumn fx:id="itemPriceColumn" prefWidth="149.0" text="Price:" />
              </columns>
            </TableView>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
