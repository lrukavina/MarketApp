<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sample.ManageUsersController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
          <top>
              <fx:include fx:id="menuBar" source="menuBar.fxml" />
          </top>
         <bottom>
            <TableView fx:id="userTableView" prefHeight="180.0" prefWidth="800.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="userNameColumn" prefWidth="226.0" text="Name:" />
                  <TableColumn fx:id="userSurnameColumn" prefWidth="186.0" text="Surname:" />
                <TableColumn fx:id="userTypeColumn" prefWidth="146.0" text="Type:" />
                  <TableColumn fx:id="userUsernameColumn" prefWidth="241.0" text="Username:" />
              </columns>
            </TableView>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
